<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>DripTock</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>DripTock</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>

	<!-- Adding LSApplicationQueriesSchemes -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
      <string>sms</string>
      <string>tel</string>
    </array>

	<key>UISupportedInterfaceOrientations</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
    </array>

	<key>NSAppleIDUsageDescription</key>
    <string>We use your Apple ID to authenticate.</string>



	<!-- Permissions for Photo Library, Camera, and Microphone -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>DripTock requires access to your photo library so you can upload pictures and videos of your styles, share them with others, and explore new fashion inspirations.</string>

	<key>NSCameraUsageDescription</key>
	<string>DripTock requires access to your camera to allow you to capture and share your styles with others in the community.</string>

	<key>NSMicrophoneUsageDescription</key>
	<string>DripTock requires access to your microphone to record audio for videos.</string>

	<!-- Additional Privacy Permissions -->
	<key>Privacy - Camera Usage Description</key>
	<string>We need access to your camera to allow you to take photos and videos of your outfits or styles and share them with other users in the app.</string>

	<key>Privacy - Microphone Usage Description</key>
	<string>DripTock needs access to your microphone for recording audio in videos.</string>
 
	<key>NSPhotoLibraryAddUsageDescription</key>
    <string>We need access to your photo library to save images.</string>
	
	<key>NSAppleMusicUsageDescription</key>
	<string>DripTock requires access to your Apple Music library to provide personalized music experiences.</string>


</dict>
</plist>
